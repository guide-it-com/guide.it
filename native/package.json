{
  "name": "native",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "dependencies": {
    "@react-navigation/native": "~7.1.18",
    "@react-navigation/native-stack": "~7.2.1",
    "@react-navigation/stack": "~7.2.10",
    "expo": "~52.0.38",
    "expo-image": "~2.0.6",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "frontend": "workspace:*",
    "react": "~18.3.1",
    "react-native": "~0.76.7",
    "react-native-fast-shadow": "~0.1.1",
    "react-native-gesture-handler": "~2.25.0",
    "react-native-macos": "~0.76.0-0",
    "react-native-reanimated": "~3.17.3",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "~15.9.0",
    "react-native-windows": "~0.76.0",
    "solito": "4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@react-native-community/cli-platform-ios": "https://github.com/react-native-community/cli#workspace=@react-native-community/cli-platform-ios",
    "@react-native/metro-config": "~0.76.0",
    "@types/node": "17.0.21",
    "@types/react": "~18.3.12",
    "babel-plugin-react-compiler": "19.0.0-beta-3229e95-20250315",
    "forever": "^4.0.3",
    "jest": "29.7.0",
    "jest-pnp-resolver": "^1.2.3",
    "jest-tsd-transform": "^1.1.1",
    "metro-config": "~0.76.4",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "react-compiler-runtime": "19.0.0-beta-3229e95-20250315",
    "ts-node": "^10.9.1",
    "typescript": "~5.3.3"
  },
  "scripts": {
    "build": "ts-node ./build.ts",
    "watch": "nodemon --watch ../web/src/pages -e ts,tsx --exec \"yarn build\"",
    "start": "expo start",
    "rnstart": "react-native start",
    "run:android": "expo run:android",
    "run:ios": "expo run:ios",
    "run:macos": "react-native run-macos && yarn rnstart",
    "run:windows": "react-native run-windows",
    "android": "yarn build && run-p watch run:android",
    "ios": "yarn build && run-p watch run:ios",
    "macos": "yarn build && run-p watch run:macos",
    "test": "node ../utils/tests/run.js",
    "windows": "yarn build && run-p watch run:windows"
  },
  "expo": {
    "install": {
      "exclude": [
        "react"
      ]
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "react-native-windows": {
    "init-windows": {
      "name": "native",
      "namespace": "native",
      "template": "old/uwp-cpp-app"
    }
  }
}
